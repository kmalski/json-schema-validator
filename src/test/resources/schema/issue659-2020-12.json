{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "object",
  "properties": {
    "states": {
      "type": "array",
      "items": {
        "type": "object",
        "title": "state",
        "properties": {
          "status": {
            "type": "string",
            "enum": [
              "WAITING",
              "RUNNING"
            ]
          }
        }
      }
    }
  }
}